#!/usr/bin/env bash
#
# Script to start offlineimap, nextcloud and bluetooth


start_offlineimap() {
        systemctl --user start offlineimap
}
        
start_bluetooth() {
        sudo systemctl start bluetooth
}

start_dropbox() {
        nextcloud start
}

main() {
    start_offlineimap &
    start_nextcloud
    start_dropbox &
}

main
